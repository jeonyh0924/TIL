    "    \n",
    "    a, b = 0, 1\n",
    "    \n",
    "    for i in range(1, n+1):\n",
    "        a, b = b, a+b\n",
    "        \n",
    "    return a%1_000_000_007\n",
    "\n",
    "print(fibonacci(int(input())))"
