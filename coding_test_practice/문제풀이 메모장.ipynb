   "source": [
    "n = int(input())\n",
    "lst = list(map(int, input().split()))\n",
    "\n",
    "answer = [-1] * n\n",
    "stack =[]\n",
    "\n",
    "for i in range(n):\n",
    "    while stack and lst[stack[-1]] < lst[i]:\n",
    "        answer[stack.pop()] = lst[i]\n",
    "    stack.append(i)\n",
    "\n",
    "print(' '.join(map(str, answer)))"
   ]
